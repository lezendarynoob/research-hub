<%- include("partials/header.ejs") %>
    <div class="row">
        <%- include("partials/admin_navbar.ejs") %>
            <div class="col-md-8 left">
                <p class="heading-pg">Targets</p>

                <div class="table-responsive">
                    <table class="table table-hover css-serial table-striped">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th scope="col">Student Name</th>
                                <th scope="col">Enrollment Number</th>
                                <th scope="col">Semester</th>
                                <th scope="col">Program</th>
                                <th scope="col">Category</th>
                                <th scope="col">Author Name</th>
                                <th scope="col">Publication Title</th>
                                <th scope="col">Journal Name</th>
                                <th scope="col">Volume Number</th>
                                <th scope="col">Issue Number</th>
                                <th scope="col">Page Number</th>
                                <th scope="col">ISSN Number</th>
                                <th scope="col">Indexing</th>
                            </tr>
                        </thead>
                        <% sPapers.forEach(function(i){ %>
                            <tbody>
                                <tr>
                                    <td></td>

                                    <td>
                                        <%= i.studentName %>
                                    </td>
                                    <td>
                                        <%= i.enrollmentNum %>
                                    </td>
                                    <td>
                                        <%= i.semester %>
                                    </td>
                                    <td>
                                        <%= i.program %>
                                    </td>
                                    <td>
                                        <%= i.category %>
                                    </td>
                                    <td>
                                        <%= i.authorName %>
                                    </td>
                                    <td>
                                        <%= i.publicationTitle %>
                                    </td>
                                    <td>
                                        <%= i.journalName %>
                                    </td>
                                    <td>
                                        <%= i.volumeNum %>
                                    </td>
                                    <td>
                                        <%= i.issueNum %>
                                    </td>
                                    <td>
                                        <%= i.pageNum %>
                                    </td>
                                    <td>
                                        <%= i.issnNum %>
                                    </td>
                                    <td>
                                        <%= i.indexing %>
                                    </td>
                                    <td>
                                        <a href="studpub/edit?puid=<%= i._id %>">Edit</a>
                                    </td>
                                    <td>
                                        <a href="#" style="color: red;">Delete</a>
                                    </td>
                                </tr>
                            </tbody>
                            <% }); %>
                    </table>
                </div>
            </div>

    </div>
    <%- include("partials/footer.ejs") %>